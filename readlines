 # practice/myfile.txt
 #  98,97,95
 # 86,74,86
 # 76,97,85


f=open('practice/myfile.txt','r')
i=0
while True:
  i=i+1
  line=f.readline()
  if not line:
    break
  m1=int(line.split(",")[0])
  m2=line.split(",")[1]
  m3=int(line.split(",")[2])
  print(f"marks of student {i} in maths is{m1*2}")
  print(f"marks of student {i} in maths is {m2*2}")
  print(f"marks of student {i} in maths is {m3}")
